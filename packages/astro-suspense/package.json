{
  "name": "astro-suspense",
  "version": "0.1.3",
  "type": "module",
  "description": "Out of order streaming support for Astro.",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch --preserveWatchOutput"
  },
  "keywords": [
    "astro",
    "suspense",
    "async",
    "streaming",
    "astro-integration"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/charlie-hadden/astro-suspense.git"
  },
  "author": "Charlie Hadden <charlie@hadden.me>",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "default": "./dist/middleware.js"
    },
    "./components": "./components/index.js"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.3.1",
    "astro": "^4.0.5",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "astro": "^4.0.5"
  },
  "files": [
    "./LICENSE",
    "./dist",
    "./components"
  ],
  "publishConfig": {
    "access": "public"
  }
}
